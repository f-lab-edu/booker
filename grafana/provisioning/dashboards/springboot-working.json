          "mappings": [
            {
              "type": "value",
              "value": "1",
              "text": "UP"
            },
            {
              "type": "value",
              "value": "0", 
              "text": "DOWN"
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
